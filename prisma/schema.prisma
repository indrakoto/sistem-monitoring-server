generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  createdAt DateTime @default(now())
  
  @@map("users")
}

model Server {
  id        Int      @id @default(autoincrement())
  name      String
  url_api   String
  createdAt DateTime @default(now())
  
  @@map("servers")
}